{
	"name": "rpc-gateway",
	"version": "0.0.0",
	"private": true,
	"types": "./types/router.d.ts",
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev --remote",
		"test": "vitest",
		"dts": "tsc -p ./tsgenconfig.json",
		"typegen": "wrangler types --env-interface CloudflareEnv env.d.ts && tsc --noEmit false",
		"db:migrations:create": "wrangler d1 migrations create venli-dev-db",
		"db:migrations:list": "wrangler d1 migrations list venli-dev-db",
		"db:migrations:apply": "wrangler d1 migrations apply venli-dev-db",
		"db:generate": "drizzle-kit generate"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.6.4",
		"@cloudflare/workers-types": "^4.20250224.0",
		"drizzle-kit": "^0.30.5",
		"typescript": "^5.5.2",
		"vitest": "~2.1.9",
		"wrangler": "^3.111.0"
	},
	"dependencies": {
		"@trpc/server": "^10.45.2",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.40.0",
		"drizzle-zod": "^0.7.0",
		"superjson": "^2.2.2",
		"zod": "^3.24.2"
	}
}
