{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["DEEP_ENV_SECRET"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "deploy": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "persistent": true
    },
    "lint": {}
  }
}