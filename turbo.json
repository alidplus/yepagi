{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "envMode": "loose",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "deploy": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "persistent": true
    },
    "lint": {}
  }
}